    "rdf",
    "linked",
    "data"
  ],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdf-ext/rdf-parser-n3/issues"
  },
  "homepage": "https://github.com/rdf-ext/rdf-parser-n3",
  "dependencies": {
    "babelify": "^7.3.0",
    "n3": "^0.4.5",
    "rdf-data-model": "git://github.com/rdf-ext/rdf-data-model.git#develop",

    "rdf-source": "git://github.com/rdf-ext/rdf-source.git#develop"
  },
  "devDependencies": {
    "es6-promise": "^3.2.1",
    "mocha": "^2.5.3",
    "rdf-ext": "git://github.com/rdf-ext/rdf-ext.git#develop",
    "rdf-ext-browser-test": "^0.9.0",
    "rdf-test-data": "git://github.com/rdf-ext/rdf-test-data.git#develop",
    "string-to-stream": "^1.1.0"
  },
  "browserify": {
    "transform": [
      [
